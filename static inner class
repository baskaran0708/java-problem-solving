//static inner class
class outerclass{
    static int a=66;
    int b=77;
    static class innerclass
    {
         void display(){
            System.out.println("A:"+a);
        }
    }
        
    }
public class staticinerclass{
    public static void main(String args[]){
        outerclass.innerclass oj=new outerclass.innerclass();
        oj.display();
    }
}
