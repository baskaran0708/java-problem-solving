abstract class shape{
   abstract void draw();
    void mess(){
        System.out.println("Message from Shape");
    }
}
class react extends shape{
//over ride
    void draw(){
        System.out.println("draw a reactange shape");
    }
    
}
public class main{
    public static void main(String args[]){
        react obj=new react();
        obj.draw();
        obj.mess();
        
        
    }
}
//Example: 2
abstract class mobile{
   abstract void camera();
   abstract void touch();
    void voicedef(){
        System.out.println("yo can make a call");
    }
}
class realme extends mobile{
    void camera(){
        System.out.println("its provide better camera");
    }
    //over ride
    void touch(){
        System.out.println("it provide amolad display");
    }
    
}
class poco extends mobile{
    void camera(){
        System.out.println("its provide 200mpx camera");
    }
    void touch(){
        System.out.println("its provide indisplay fingerprint");
    }
}
public class main{
    public static void main(String args[]){
        poco obj=new poco();
        obj.camera();
        obj.touch();
        realme obj1=new realme();
        obj1.camera();
        obj1.touch();
        
        
        
    }
}
